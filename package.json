{
  "name": "slack-recombine",
  "version": "0.1.0",
  "scripts": {
    "prebuild": "npm run clean",
    "prestart": "npm run clean",
    "build": "bsb -make-world",
    "bundle": "browserify lib/js/src/App.js --node --standalone handler -g uglifyify > handler.js",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "local": "AWS_REGION=us-east-1 sls offline start",
    "deploy": "sls deploy -v"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/bs-json": "^5.0.2",
    "axios": "^0.21.2",
    "browserify": "^16.5.2",
    "bs-axios": "0.0.43",
    "bs-platform": "^8.2.0",
    "bsb-js": "^1.1.7",
    "serverless": "^1.82.0",
    "serverless-offline": "^6.7.0",
    "serverless-prune-plugin": "^1.4.3",
    "uglifyify": "^5.0.2"
  }
}
